{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  },
  "scripts": {
    "tailwind": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "browser-sync": "browser-sync start --server --startPath /src /index.html --files 'src/*.html,*.css,*.js'",
    "start": "npm-run-all --parallel tailwind browser-sync"
  },
  "dependencies": {
    "daisyui": "^2.41.0"
  }
}
